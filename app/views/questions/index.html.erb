<h1>Questions</h1>

<% if @questions.any? %>
<ul>
  <% @questions.each do |question| %>
  <li><%= link_to question.title, question_path(question) %></li>
  <% end %>
</ul>
<% else %>
<p>There are no questions yet.</p>
<% end %>

<p><%= link_to "Add question", new_question_path %></p>
<p><%= link_to "See User List", users_path %></p>
